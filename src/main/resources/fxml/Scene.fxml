<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #909EA3;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.altcolorlab.rollcall.FXMLController">
   <children>
      <Label contentDisplay="CENTER" layoutX="22.0" layoutY="114.0" prefHeight="27.0" prefWidth="84.0" text="Job Number:">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tfRollNum" onKeyPressed="#handleEnterPressed" layoutX="106.0" layoutY="115.0" prefHeight="25.0" prefWidth="145.0" />
      <Button fx:id="searchButton" alignment="CENTER" layoutX="271.0" layoutY="115.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="99.0" text="Search">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
         <effect>
            <DropShadow blurType="TWO_PASS_BOX" color="#00000089" />
         </effect>
      </Button>
      <Label fx:id="searchMessage" contentDisplay="CENTER" layoutX="15.0" layoutY="164.0" prefHeight="135.0" prefWidth="358.0">
         <font>
            <Font name="Georgia" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
